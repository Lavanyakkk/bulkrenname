import os
def rename():
   i=1
   path='C:\\Users\\lavanya\\PycharmProjects\\project\\pictures\\'
   for filename in os.listdir(path):
       picturenames=f"pic{i}.jpg"
       src=path+filename
       dest=path+picturenames
       os.rename(src,dest)
       i=i+1
if __name__=="__main__":
   rename() 
